	public static void RndMeter(ArrayList<Integer> list) {
		Random rnd = new Random();

		int l = rnd.nextInt(9,20);
		for (int i = 0; i <= l; i++) {
			int num = rnd.nextInt(-1,100);
			list.add(num);

		}
